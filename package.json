{
  "name": "@tipe/cli",
  "version": "1.11.1",
  "description": "Tipe CLI",
  "main": "./lib/index.js",
  "bin": {
    "tipe": "./bin/tipe"
  },
  "files": [
    "bin/**/*",
    "lib/**/*"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --detectOpenHandles --forceExit",
    "lint": "eslint src",
    "lint-autofix": "yarn lint --fix",
    "precommit": "lint-staged && yarn test --passWithNoTests",
    "ci": "yarn lint && yarn test",
    "build": "babel src -d lib",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release",
    "dev": "yarn build && node lib/index.js"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tipeio/tipe-cli.git"
  },
  "author": "@tipe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tipeio/tipe-cli/issues"
  },
  "homepage": "https://github.com/tipeio/tipe-cli#readme",
  "dependencies": {
    "@tipe/schema-tools": "^2.5.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.6",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "boxen": "^4.1.0",
    "caporal": "^1.3.0",
    "cascade-config": "^1.4.5",
    "chalk": "^2.4.2",
    "chance": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "lodash": "^4.17.15",
    "log-symbols": "^3.0.0",
    "morgan": "^1.9.1",
    "nanoid": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@tipe/eslint-config-tipe": "^2.1.0",
    "@tipe/js": "^2.1.1",
    "@tipe/schema": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8",
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "5.15.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "globby": "^8",
    "husky": "^0.14.3",
    "jest": "^24.5.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1"
  },
  "engines": {
    "node": ">=8"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        },
        {
          "type": "chore",
          "scope": "deps",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    }
  }
}
