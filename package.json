{
  "name": "@tipe/cli",
  "version": "0.0.3",
  "description": "Tipe CLI",
  "main": "dist/index.js",
  "bin": {
    "tipe": "./bin/run"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "test": "NODE_ENV=testing jest --detectOpenHandles --forceExit",
    "lint": "eslint --ext .js, src",
    "lint-autofix": "yarn lint --fix",
    "precommit": "lint-staged && yarn test",
    "ci": "yarn lint && yarn test",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tipeio/tipe-cli.git"
  },
  "author": "@tipe",
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "tipe",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "track": "./dist/hooks/track"
    }
  },
  "files": [
    "/bin",
    "/dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://github.com/tipeio/tipe-cli/issues"
  },
  "homepage": "https://github.com/tipeio/tipe-cli#readme",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@tipe/schema-tools": "^2.5.0",
    "apollo-server": "^2.0.0",
    "chance": "^1.0.16",
    "cli-ux": "^4.8.0",
    "commander": "^2.14.1",
    "express": "^4.16.3",
    "got": "^9.2.2",
    "graphql": "^0.13.2",
    "graphql-import": "^0.7.1",
    "lodash": "^4.17.10",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.1",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@tipe/eslint-config-tipe": "^2.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "globby": "^8",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "nock": "^10.0.1",
    "prettier": "^1.14.0",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        },
        {
          "type": "chore",
          "scope": "deps",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    }
  }
}
