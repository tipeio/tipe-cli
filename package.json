{
  "name": "@tipe/cli",
  "version": "0.0.3",
  "description": "Tipe CLI",
  "main": "src/index.js",
  "bin": {
    "tipe": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test:mocha": "nyc mocha --forbid-only --exit \"test/commands/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint src/**",
    "test:jest": "jest ./test/jest/**",
    "ci": "npm run lint && npm run test:jest && npm run test:mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tipeio/tipe-cli.git"
  },
  "author": "@tipe",
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "tipe",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "bugs": {
    "url": "https://github.com/tipeio/tipe-cli/issues"
  },
  "homepage": "https://github.com/tipeio/tipe-cli#readme",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@tipe/schema-tools": "^1.0.1",
    "apollo-server": "^2.0.0",
    "camel-case": "^3.0.0",
    "chance": "^1.0.16",
    "change-case": "^3.0.2",
    "cli-ux": "^4.8.0",
    "commander": "^2.14.1",
    "configstore": "^3.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "fs.promised": "^3.0.0",
    "graphql": "^0.13.2",
    "graphql-parse-resolve-info": "^4.0.0-rc.4",
    "inquirer": "^5.1.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.10",
    "node-fetch": "^2.2.0",
    "pascal-case": "^2.0.1",
    "pluralize": "^7.0.0",
    "prompt": "^1.0.0",
    "shelljs": "^0.8.2",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@tipe/eslint-config-tipe": "^1.0.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^12",
    "chai": "^4"
  }
}
